{
  "name": "webpack-setup",
  "version": "0.0.1",
  "description": "Basic application demonstrating Webpack setup with vue",
  "scripts": {
    "build-dev": "webpack --progress --colors --watch --env.development",
    "build-prod": "NODE_ENV=production webpack --env.production",
    "build-prod-uglify": "NODE_ENV=production webpack --env.production --env.uglify",
    "build-dev-uglify": "webpack --progress --colors --watch --env.development --env.uglify"
  },
  "author": "Trekels Vincent",
  "license": "none",
  "dependencies": {
    "classname": "0.0.0",
    "classnames": "^2.2.6",
    "file-loader": "^2.0.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^7.1.0",
    "vue": "^2.5.17",
    "vue-clickaway": "^2.2.2",
    "vue-loader": "^15.4.2",
    "vue-numeric": "^2.3.0",
    "vue-router": "^3.0.1",
    "vue-tweet-embed": "^2.3.0",
    "vuex": "^3.1.0",
    "webpack": "^2.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.11",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "vue-hot-reload-api": "^2.3.0",
    "vue-html-loader": "^1.2.4",
    "vue-template-compiler": "^2.5.17"
  }
}
